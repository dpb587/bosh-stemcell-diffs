#!/bin/bash

set -e

URL="https://d26ekeud912fhb.cloudfront.net/bosh-stemcell/$STEMCELL_INFRASTRUCTURE/bosh-stemcell-$STEMCELL_VERSION-$STEMCELL_INFRASTRUCTURE-$STEMCELL_HYPERVISOR-$STEMCELL_OS_NAME-$STEMCELL_OS_VERSION-go_agent.tgz"
FILE_NAME=$( basename "$URL" )
FILE_PATH="stemcells/source/$FILE_NAME"

if [ ! -e "stemcells/source/$FILE_NAME" ] ; then
  wget -qO "$FILE_PATH" "$URL"
fi
